<p id="notice"><%= notice %></p>

<h1>Releases</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Catalogue number</th>
      <th>Format</th>
      <th>Country code</th>
      <th>Released at</th>
      <th>Image data</th>
      <th>Artist</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @releases.each do |release| %>
      <tr>
        <td><%= release.title %></td>
        <td><%= release.catalogue_number %></td>
        <td><%= release.format %></td>
        <td><%= release.country_code %></td>
        <td><%= release.released_at %></td>
        <td><%= release.image_data %></td>
        <td><%= release.artist %></td>
        <td><%= link_to 'Show', release %></td>
        <td><%= link_to 'Edit', edit_release_path(release) %></td>
        <td><%= link_to 'Destroy', release, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Release', new_release_path %>
